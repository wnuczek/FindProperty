-- Table structure for objects
  CREATE TABLE IF NOT EXISTS objects (
    id int(11) NOT NULL,
    `name` varchar(200) NOT NULL,
    `owner` varchar(200) NOT NULL,
	companyType varchar(200) NOT NULL,
    transactionType varchar(200) NOT NULL,
    objectType varchar(200) NOT NULL,
    objectLocation varchar(200) NOT NULL,
    objectArea DECIMAL NOT NULL,
    objectPrice DECIMAL NOT NULL,
    created_at datetime NOT NULL DEFAULT CURRENT_TIMESTAMP
  ) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;
  ALTER TABLE objects ADD PRIMARY KEY (id);
  ALTER TABLE objects MODIFY id int(11) NOT NULL AUTO_INCREMENT;

  INSERT INTO objects (id, name, owner, companyType, transactionType, objectType, objectLocation, objectArea, objectPrice, created_at) VALUES 
  (1, 'Title placeholder', 3, 'Company placeholder', 'sprzedaż', 'dom', 'Warszawa', 250, 1750000, '2019-02-28 13:20:20'),
  (2, 'Title placeholder', 4, 'Company placeholder', 'wynajem', 'biuro', 'Warszawa', 450, 30000, '2019-02-28 13:20:20'),
  (3, 'Title placeholder', 5, 'Company placeholder', 'wynajem', 'lokal usługowy', 'Warszawa', 150, 9000, '2019-02-28 13:20:20');

-- Table structure for objects-searched
  CREATE TABLE IF NOT EXISTS `objects-searched` (
    id int(11) NOT NULL,
    `name` varchar(200) NOT NULL,
    `owner` varchar(200) NOT NULL,
    companyType varchar(200) NOT NULL,
    transactionType varchar(200) NOT NULL,
    objectType varchar(200) NOT NULL,
    objectLocation varchar(200) NOT NULL,
    objectAreaMin DECIMAL NOT NULL,
    objectAreaMax DECIMAL NOT NULL,
    objectPriceMin DECIMAL NOT NULL,
    objectPriceMax DECIMAL NOT NULL,
    created_at datetime NOT NULL DEFAULT CURRENT_TIMESTAMP
  ) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;
  ALTER TABLE `objects-searched` ADD PRIMARY KEY (id);
  ALTER TABLE `objects-searched` MODIFY id int(11) NOT NULL AUTO_INCREMENT;

  INSERT INTO `objects-searched` (id, name, owner, companyType, transactionType, objectType, objectLocation, objectAreaMin, objectAreaMax, objectPriceMin, objectPriceMax, created_at) VALUES 
